//solution 1
class Solution {
    public int singleNumber(int[] nums) {
        Set<Integer> set = new HashSet();
        for (int i: nums){
            if(set.contains(i)){
                set.remove(i);
            } else {
                set.add(i);
            }
        }
        Integer[] arr = new Integer[1];
        arr = set.toArray(arr);
        return arr[0];
    }
}

//solution 2
class Solution {
    public int singleNumber(int[] nums) {
        int ans = 0;
        for (int i: nums){
            ans ^= i;
        }
        return ans;
    }

}
