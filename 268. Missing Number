//solution1
class Solution {
    public int missingNumber(int[] nums) {
        HashSet<Integer> set = new HashSet();
        for(int i = 0; i <= nums.length; i++){
            set.add(i);
        }
        for(int j = 0; j < nums.length; j++){
            set.remove(nums[j]);
        }
        return set.toArray(new Integer[1])[0];
    }
}
//solution2
class Solution {
    public int missingNumber(int[] nums) {
        int ans = nums.length;
        for(int i = 0; i < nums.length; i++){
            ans  ^= i^nums[i];
        }
        return ans;
    }
}
