//solution 1
class Solution {
    public int findDuplicate(int[] nums) {
        Set<Integer> set = new HashSet();
        for(int i: nums) {
            if (!set.add(i)){
                return i;
            }
        }
        return -1;
    }
}
//solution 2
class Solution {
    public int findDuplicate(int[] nums) {
        Arrays.sort(nums);
        for(int i = 1; i < nums.length; i++){
            if(nums[i] ==nums[i - 1]) 
                return nums[i];
        }
        return -1;
    }

}
