class Solution {
    public boolean isValidSudoku(char[][] board) {
        Set<String> check = new HashSet();
        for(int i = 0;i < 9; i++) {
            for(int j = 0; j < 9; j++) {
                char v = board[i][j]
                if(v =='.') {
                    continue;
                }
                if(!check.add(v +"in row "+ i) || !check.add(v +"in col "+ j) || !check.add(v +"in block "+ i /3 +"-"+ j/3)){
                    return false;
                }
            }
        }
        return true;
    }
}
