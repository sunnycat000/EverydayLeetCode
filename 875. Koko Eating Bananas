class Solution {
    public int minEatingSpeed(int[] piles, int h) {
        int start = 1, end = 1000000000;
        while(start < end) {
            int mid = (end - start) / 2 + start;
            int hour = 0;
            for (int i = 0; i < piles.length; i++){
                hour += (piles[i] + mid - 1)/mid;
            }
            if (hour > h) {
                start = mid + 1;
            } else {
                end = mid;
            } 
        }
        return end;
    }
}
